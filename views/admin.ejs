<%- include("header") %>

<div id="container">
    <%- include("menu") %> 
    <div id="content">
        <h1>Admin oldal</h1>
        <p>Itt lesz majd látható az összes ember profil adata!</p>
        <table id="myTable">
            <% for (let i = 0; i < field.length; i++) { %>
                <% if (i != 3) { %>
                    <th><%= field[i].name %> </th>
                <% } %> 
            <% } %>
            <th>edit</th>
            <% for (let i = 0; i < users.length; i++) { %>
                <tr>
                <% for (let j = 0; j < field.length; j++) { %>
                    <% if (j != 3) { %>
                        <td><%= Object.values(users[i])[j] %> </td>
                    <% } %> 
                   
                <% } %>
                <td><a href="/admin/<%= users[i].ID %>">Edit</a></td>
                </tr>
            <% } %>
    
        </table>
    </div>
</div>

<%- include("footer") %>